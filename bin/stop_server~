BASEDIR=$(dirname $0)
cd $BASEDIR
file=../var/kvpaxos_$1.pid
if [ $1 == '-p' ]; then
	ppid=$(cat $pfile)
	kill $ppid
elif [ $1 == '-b' ]; then
	bpid=$(cat $bfile)
	kill $bpid
elif [ $1 == '-a' ]; then
	ppid=$(cat $pfile)
	bpid=$(cat $bfile)
	kill $ppid
	kill $bpid
else
	echo "usage: stop_server"
fi

